#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/xriot/Projects/JavaScript/Frontend/kasirku_frontend_web/node_modules/.pnpm/react-email@3.0.1_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/react-email/dist/cli/node_modules:/home/xriot/Projects/JavaScript/Frontend/kasirku_frontend_web/node_modules/.pnpm/react-email@3.0.1_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/react-email/dist/node_modules:/home/xriot/Projects/JavaScript/Frontend/kasirku_frontend_web/node_modules/.pnpm/react-email@3.0.1_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/react-email/node_modules:/home/xriot/Projects/JavaScript/Frontend/kasirku_frontend_web/node_modules/.pnpm/react-email@3.0.1_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules:/home/xriot/Projects/JavaScript/Frontend/kasirku_frontend_web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/xriot/Projects/JavaScript/Frontend/kasirku_frontend_web/node_modules/.pnpm/react-email@3.0.1_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/react-email/dist/cli/node_modules:/home/xriot/Projects/JavaScript/Frontend/kasirku_frontend_web/node_modules/.pnpm/react-email@3.0.1_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/react-email/dist/node_modules:/home/xriot/Projects/JavaScript/Frontend/kasirku_frontend_web/node_modules/.pnpm/react-email@3.0.1_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/react-email/node_modules:/home/xriot/Projects/JavaScript/Frontend/kasirku_frontend_web/node_modules/.pnpm/react-email@3.0.1_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules:/home/xriot/Projects/JavaScript/Frontend/kasirku_frontend_web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-email/dist/cli/index.js" "$@"
else
  exec node  "$basedir/../react-email/dist/cli/index.js" "$@"
fi
